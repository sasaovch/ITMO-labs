<!--?xml version='1.0' encoding='UTF-8'?-->
<faces-config
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
        version="2.0">

     <managed-bean>
        <managed-bean-name>attempt</managed-bean-name>
        <managed-bean-class>com.lab3.facelets.Attempt</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
     </managed-bean> 

    <managed-bean>
        <managed-bean-name>dbManager</managed-bean-name>
        <managed-bean-class>com.lab3.facelets.DbManager</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
     </managed-bean>

</faces-config>